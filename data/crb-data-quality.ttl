:concisenessMetric
    a dqv:Metric ;
    skos:prefLabel "Conciseness" ;
    skos:definition "It refers to the minimisation of redundancy of entities at the schema"@en ;
    dqv:inDimension ldqd:conciseness ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "PREFIX owl: <http://www.w3.org/2002/07/owl#> SELECT ?o ?type (COUNT(?s) AS ?total) FROM <http://data.ascdc.tw/Book/crb/> WHERE {{?s a ?type . ?s owl:sameAs ?o }} GROUP BY ?o ?type HAVING(COUNT(?s)>1) LIMIT {0}" ;
    schema:description "It refers to the minimisation of redundancy of entities at the schema"
    .        
    
