    dcterms:issued "2021-04-01"^^xsd:date;
    dcterms:license <http://www.opendefinition.org/licenses/cc-zero>;
    void:feature <http://www.w3.org/ns/formats/RDF_XML>;
    void:sparqlEndpoint <https://data.ascdc.tw/sparql>;
    wdt:P5305 <https://data.ascdc.tw/sparql>;
    void:vocabulary <http://id.loc.gov/ontologies/bibframe/>;
    dqv:hasQualityMeasurement :myCRBMeasurementAvailability ;
    dqv:hasQualityMeasurement :myCRBMeasurementLicensing ;
    dqv:hasQualityMeasurement :myCRBMeasurementInterlinkingPerson ;
    dqv:hasQualityMeasurement :myCRBMeasurementInterlinkingWork ;
    dqv:hasQualityMeasurement :myCRBMeasurementInterlinkingWikidata ;  
    dqv:hasQualityMeasurement :myCRBMeasurementSecurity ; 
    dqv:hasQualityMeasurement :myCRBMeasurementPerformance ;
    dqv:hasQualityMeasurement :myCRBMeasurementSyntactic ;
    dqv:hasQualityMeasurement :myCRBMeasurementConciseness ;
    dqv:hasQualityMeasurement :myCRBMeasurementTrustworthiness ;
    dqv:hasQualityMeasurement :myCRBMeasurementUnderstandabilityLabels ;
    dqv:hasQualityMeasurement :myCRBMeasurementUnderstandabilityVocabularies ;
    dqv:hasQualityMeasurement :myCRBMeasurementUnderstandabilityURIPatterns ;
    dqv:hasQualityMeasurement :myCRBMeasurementUnderstandabilityExamples ;
    dqv:hasQualityMeasurement :myCRBMeasurementUnderstandabilityDatasheets ;
    dqv:hasQualityMeasurement :myCRBMeasurementTimeliness ;
    dqv:hasQualityMeasurement :myCRBMeasurementRepresentationalConcisenessURIsLength ;
    dqv:hasQualityMeasurement :myCRBMeasurementRepresentationalConcisenessContainers ;
    dqv:hasQualityMeasurement :myCRBMeasurementInteroperability ;
    dqv:hasQualityMeasurement :myCRBMeasurementInterpretabilityViaf ;
    dqv:hasQualityMeasurement :myCRBMeasurementInterpretabilityIsni ;
    dqv:hasQualityMeasurement :myCRBMeasurementInterpretabilityLabels ;
