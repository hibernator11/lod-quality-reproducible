    .
    
:org_Crb a foaf:Organization;
    rdfs:label "Fu Ssu-nien Library of the Institute of History and Philology, Academia Sinica" ;
    foaf:homepage <http://rarebookdl.ihp.sinica.edu.tw/rarebook/Search/index.jsp> ;
    .

:availabilityMetric
    a dqv:Metric ;
    skos:prefLabel "Availability" ;
    skos:definition "It checks that the SPARQL endpoint is available"@en ;
    dqv:inDimension ldqd:availability ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "SELECT * FROM <http://data.ascdc.tw/Book/crb/> WHERE {{?s ?p ?o}} LIMIT {0}" ;
    schema:description "It checks that the SPARQL endpoint is available" 
    .

:myCRBMeasurementAvailability 
    a dqv:QualityMeasurement ;
    dqv:computedOn :myCRBDataset ;
    dqv:isMeasurementOf :availabilityMetric ;
    dqv:value "1"^^xsd:decimal 
    .

:licensingMetric
    a dqv:Metric ;
    skos:prefLabel "Licensing" ;
    skos:definition "It checks the inclusion of machine-readable metadata concerning the licence"@en ;
    dqv:inDimension ldqd:licensing ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "PREFIX dcterms: <http://purl.org/dc/terms/> PREFIX schema: <https://schema.org/> SELECT * FROM <http://data.ascdc.tw/Book/crb/> WHERE {{{{?s dcterms:license ?o}} UNION {{?s schema:licence ?o}}}} LIMIT {0}" ;
    schema:description "It checks the inclusion of machine-readable metadata concerning the licence" 
    .

:myCRBMeasurementLicensing 
    a dqv:QualityMeasurement ;
    dqv:computedOn :myCRBDataset ;
    dqv:isMeasurementOf :licensingMetric ;
    dqv:value "0.5"^^xsd:decimal 
    .
    
