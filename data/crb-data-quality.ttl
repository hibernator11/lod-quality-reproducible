    schema:query "PREFIX foaf: <http://xmlns.com/foaf/0.1/> SELECT ?p WHERE{{{{ SELECT distinct ?p FROM <http://data.ascdc.tw/Book/crb/> WHERE {{{{ ?s ?p ?o }}}} LIMIT 100 }} FILTER (strstarts(str(?p), 'http://www.europeana.eu/schemas/edm/') || strstarts(str(?p), 'http://purl.org/dc/terms/') || strstarts(str(?p), 'http://xmlns.com/foaf/0.1/') || strstarts(str(?p), 'https://schema.org')) }} LIMIT {0} " ;
    schema:description "It checks the use of existing vocabularies to describe the metadata"
