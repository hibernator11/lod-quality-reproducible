:interpretabilityViafMetric
    a dqv:Metric ;
    skos:prefLabel "Interpretability-VIAF" ;
    skos:definition "It checks the use of correct uris for the viaf resources"@en ;
    dqv:inDimension ldqd:interpretability ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "PREFIX owl: <http://www.w3.org/2002/07/owl#> PREFIX bne: <https://datos.bne.es/def/> SELECT ?o WHERE {{{{ SELECT ?o WHERE {{ ?s a bne:C1005 . ?s owl:sameAs ?o . FILTER (strstarts(str(?o), 'http://viaf.org/viaf/')) }} LIMIT 100 }} FILTER(!regex(str(?o),'^http://viaf.org/viaf/[0-9]+')) }} LIMIT {0}" ;
    schema:description "It checks the use of correct uris for the viaf resources"
    .      
    
 :myBneMeasurementInterpretabilityIsni
