:interoperabilityMetric
    a dqv:Metric ;
    skos:prefLabel "Interoperability" ;
    skos:definition "It checks the use of existing vocabularies to describe the metadata"@en ;
    dqv:inDimension ldqd:interoperability ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "PREFIX bne: <https://datos.bne.es/def/> SELECT ?p WHERE{{{{ SELECT distinct ?p WHERE {{{{ ?s a bne:C1005.  ?s ?p ?o }}}} LIMIT 100 }} FILTER (strstarts(str(?p), 'http://www.europeana.eu/schemas/edm/') || strstarts(str(?p), 'http://purl.org/dc/terms/') || strstarts(str(?p), 'http://xmlns.com/foaf/0.1/') || strstarts(str(?p), 'https://datos.bne.es/def/')) }} LIMIT {0} " ;
    schema:description "It checks the use of existing vocabularies to describe the metadata"
    .       
    
:myBneMeasurementInterpretabilityViaf
    a dqv:QualityMeasurement ;
