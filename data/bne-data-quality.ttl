:myBneDataset a void:Dataset;
    dcterms:title "National Library of Spain";
    dcterms:description "Datos.bne.es es una plataforma de la Biblioteca Nacional de España que propone al usuario una nueva experiencia de navegación por los datos del catálogo de la BNE, de una forma completamente distinta a la tradicional";
    dcterms:creator :org_Bne;
    dcterms:issued "2013-01-01"^^xsd:date;
    dcterms:license <http://creativecommons.org/licenses/by/4.0/>;
    dcterms:source <https://doi.org/10.3233/SW-120094>;
    void:feature <http://www.w3.org/ns/formats/RDF_XML>;
    void:sparqlEndpoint <https://datos.bne.es/sparql>;
    wdt:P5305 <https://datos.bne.es/sparql>;
    void:subset :myBne_autoridades;
    void:subset :myBne_materias;
    void:subset :myBne_bibliograficos;
    void:vocabulary <https://datos.bne.es/def/>;
    void:exampleResource <https://datos.bne.es/resource/XX1718747>;
    void:exampleResource <https://datos.bne.es/resource/XX1066651>;
    void:exampleResource <https://datos.bne.es/resource/XX971832>;
    dqv:hasQualityMeasurement :myBneMeasurementAvailability ;
    dqv:hasQualityMeasurement :myBneMeasurementLicensing ;
    dqv:hasQualityMeasurement :myBneMeasurementInterlinking ;
    dqv:hasQualityMeasurement :myBneMeasurementSecurity ; 
    dqv:hasQualityMeasurement :myBneMeasurementPerformance ;
    dqv:hasQualityMeasurement :myBneMeasurementSyntactic ;
    dqv:hasQualityMeasurement :myBneMeasurementConciseness ;
    dqv:hasQualityMeasurement :myBneMeasurementTrustworthiness ;
    dqv:hasQualityMeasurement :myBneMeasurementUnderstandabilityLabels ;
    dqv:hasQualityMeasurement :myBneMeasurementUnderstandabilityVocabularies ;
    dqv:hasQualityMeasurement :myBneMeasurementUnderstandabilityURIPatterns ;
    dqv:hasQualityMeasurement :myBneMeasurementUnderstandabilityExamples ;
    dqv:hasQualityMeasurement :myBneMeasurementTimeliness ;
    dqv:hasQualityMeasurement :myBneMeasurementRepresentationalConcisenessURIsLength ;
    dqv:hasQualityMeasurement :myBneMeasurementRepresentationalConcisenessContainers ;
    dqv:hasQualityMeasurement :myBneMeasurementInteroperability ;
    dqv:hasQualityMeasurement :myBneMeasurementInterpretabilityViaf ;
    dqv:hasQualityMeasurement :myBneMeasurementInterpretabilityIsni ;
    dqv:hasQualityMeasurement :myBneMeasurementInterpretabilityLabels ;
    dqv:hasQualityMeasurement :myBneMeasurementVersatilitySerialisation ;
    dqv:hasQualityMeasurement :myBneMeasurementVersatilityMultilingual ;
    dqv:hasQualityMeasurement :myBneMeasurementRelevancy ;
    dqv:hasQualityMeasurement :myBneMeasurementConsistency ;
    dqv:hasQualityMeasurement :myBneMeasurementSemanticAccuracy ;
    dqv:hasQualityMeasurement :myBneMeasurementCompletenessPerson ;
    dqv:hasQualityMeasurement :myBneMeasurementCompletenessWork ;
    dqv:hasQualityMeasurement :myBneMeasurementCompletenessPlace ;
    dqv:hasQualityMeasurement :myBneMeasurementCompletenessPopulation
    .
   
:myBne_autoridades a void:Dataset;
    dcterms:title "Registros de autoridad";
    dcterms:date "2021-01-21"^^xsd:date;
    dcterms:description "Registros de autoridad (362MB)";
    void:dataDump <http://datos.bne.es/datadump/autoridades.nt.bz2>
    .

:myBne_materias a void:Dataset;
    dcterms:title "Encabezamientos de Materias";
    dcterms:date "2021-01-21"^^xsd:date;
    dcterms:description "Encabezamientos de Materias en SKOS (36,3MB)";
    void:dataDump <http://datos.bne.es/datadump/materias.nt.bz2>
    .    

:myBne_bibliograpficos a void:Dataset;
    dcterms:title "Registros Bibliográficos";
    dcterms:date "2021-01-21"^^xsd:date;
    dcterms:description "Registros bibliográficos (1.3GB)";
    void:dataDump <http://datos.bne.es/datadump/bibliograficos.nt.bz2>
    .
    
:org_Bne a foaf:Organization;
    rdfs:label "National Library of Spain" ;
    foaf:homepage <https://datos.bne.es/> ;
    .

:availabilityMetric
    a dqv:Metric ;
    skos:prefLabel "Availability" ;
    skos:definition "It checks that the SPARQL endpoint is available"@en ;
    dqv:inDimension ldqd:availability ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "SELECT * WHERE {{?s ?p ?o}} LIMIT {0}" ;
    schema:description "It checks that the SPARQL endpoint is available" 
    .

:myBneMeasurementAvailability 
    a dqv:QualityMeasurement ;
    dqv:computedOn :myBneDataset ;
    dqv:isMeasurementOf :availabilityMetric ;
    dqv:value "1"^^xsd:decimal 
    .

:licensingMetric
    a dqv:Metric ;
    skos:prefLabel "Licensing" ;
    skos:definition "It checks the inclusion of machine-readable metadata concerning the licence"@en ;
    dqv:inDimension ldqd:licensing ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "PREFIX dcterms: <http://purl.org/dc/terms/> PREFIX schema: <https://schema.org/> SELECT * WHERE {{{{?s dcterms:license ?o}} UNION {{?s schema:licence ?o}}}} LIMIT {0}" ;
    schema:description "It checks the inclusion of machine-readable metadata concerning the licence" 
    .

:myBneMeasurementLicensing 
    a dqv:QualityMeasurement ;
    dqv:computedOn :myBneDataset ;
    dqv:isMeasurementOf :licensingMetric ;
    dqv:value "1"^^xsd:decimal 
    .
    
:myBneMeasurementInterlinking 
    a dqv:QualityMeasurement ;
    dqv:computedOn :myBneDataset ;
    dqv:isMeasurementOf :interlinkingMetric ;
    dqv:value "1"^^xsd:decimal 
    .

:interlinkingMetric
    a dqv:Metric ;
    skos:prefLabel "Interlinking" ;
    skos:definition "It checks the extent to which resources in a repository are linked to external datasets"@en ;
    dqv:inDimension ldqd:interlinking ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "PREFIX owl: <http://www.w3.org/2002/07/owl#> SELECT * WHERE {{?s owl:sameAs ?o}} LIMIT {0}" ;
    schema:description "It checks the extent to which resources in a repository are linked to external datasets" 
    .    
    
:myBneMeasurementSecurity 
    a dqv:QualityMeasurement ;
    dqv:computedOn :myBneDataset ;
    dqv:isMeasurementOf :securityMetric ;
    dqv:value "1"^^xsd:decimal 
    .

:securityMetric
    a dqv:Metric ;
    skos:prefLabel "Security" ;
    skos:definition "It checks the extent to which data is protected against alteration and misuse"@en ;
    dqv:inDimension ldqd:security ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "PREFIX prov: <http://www.w3.org/ns/prov#> SELECT * WHERE {{{{?s prov:wasGeneratedBy ?o}} UNION {{?s a prov:Agent}}}} LIMIT {0}" ;
    schema:description "It checks the extent to which data is protected against alteration and misuse" 
    .        
    
:myBneMeasurementPerformance 
    a dqv:QualityMeasurement ;
    dqv:computedOn :myBneDataset ;
    dqv:isMeasurementOf :performanceMetric ;
    dqv:value "1"^^xsd:decimal 
    .

:performanceMetric
    a dqv:Metric ;
    skos:prefLabel "Performance" ;
    skos:definition "It checks whether the time to answer ten requests divided by ten is not longer than the time it takes to answer one request"@en ;
    dqv:inDimension ldqd:performance ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "SELECT * WHERE {{?s ?p ?o}} LIMIT {0}" ;
    schema:description "It checks whether the time to answer ten requests divided by ten is not longer than the time it takes to answer one request" 
    .            

:myBneMeasurementSyntactic 
    a dqv:QualityMeasurement ;
    dqv:computedOn :myBneDataset ;
    dqv:isMeasurementOf :syntacticMetric ;
    dqv:value "1"^^xsd:decimal 
    .

:syntacticMetric
    a dqv:Metric ;
    skos:prefLabel "Syntactic validity" ;
    skos:definition "It checks the degree to which an RDF document conforms to the specification of the serialisation format"@en ;
    dqv:inDimension ldqd:syntacticValidity ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "PREFIX cidoc: <http://www.cidoc-crm.org/cidoc-crm/> SELECT ?subject WHERE {{?subject rdf:type cidoc:E21_Person}} LIMIT {0}" ;
    schema:description "It checks the degree to which an RDF document conforms to the specification of the serialisation format" 
    .            

:myBneMeasurementConciseness 
    a dqv:QualityMeasurement ;
    dqv:computedOn :myBneDataset ;
    dqv:isMeasurementOf :concisenessMetric ;
    dqv:value "0"^^xsd:decimal 
    .

:concisenessMetric
    a dqv:Metric ;
    skos:prefLabel "Conciseness" ;
    skos:definition "It refers to the minimisation of redundancy of entities at the schema"@en ;
    dqv:inDimension ldqd:conciseness ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "PREFIX owl: <http://www.w3.org/2002/07/owl#> SELECT ?o ?type (COUNT(?s) AS ?total) WHERE {{?s a ?type . ?s owl:sameAs ?o }} GROUP BY ?o ?type HAVING(COUNT(?s)>1) LIMIT {0}" ;
    schema:description "It refers to the minimisation of redundancy of entities at the schema"
    .        
    
:myBneMeasurementTrustworthiness
    a dqv:QualityMeasurement ;
    dqv:computedOn :myBneDataset ;
    dqv:isMeasurementOf :trustworthinessMetric ;
    dqv:value "0"^^xsd:decimal 
    .

:trustworthinessMetric
    a dqv:Metric ;
    skos:prefLabel "Trustworthiness" ;
    skos:definition "It refers to the degree to which the information is accepted to be correct, true, real and credible"@en ;
    dqv:inDimension ldqd:trustworthiness ;
    dqv:expectedDataType xsd:decimal ;
    schema:query "PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> SELECT * WHERE {{?s rdfs:label ?o . FILTER (?o = 'Unknown' || ?o = 'Anonymous' || ?o = 'Unknown value')}} LIMIT {0}" ;
    schema:description "It refers to the degree to which the information is accepted to be correct, true, real and credible"
    .            
    
:myBneMeasurementUnderstandabilityLabels
    a dqv:QualityMeasurement ;
    dqv:computedOn :myBneDataset ;
    dqv:isMeasurementOf :understandabilityLabelsMetric ;
    dqv:value "0"^^xsd:decimal 
    .

:understandabilityLabelsMetric
