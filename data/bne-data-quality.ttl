     dqv:expectedDataType xsd:decimal ;
     schema:query "PREFIX owl: <http://www.w3.org/2002/07/owl#> PREFIX bne: <https://datos.bne.es/def/> SELECT ?o WHERE {{{{ SELECT ?o WHERE {{ ?s a bne:C1005 . ?s owl:sameAs ?o . FILTER (strstarts(str(?o), 'https://isni.org/isni/')) }} LIMIT 100 }} FILTER(!regex(str(?o),'^https://isni.org/isni/[0-9]{{16}}')) }} LIMIT {0}" ;
     schema:description "It checks the use of correct uris for the Isni resources"
